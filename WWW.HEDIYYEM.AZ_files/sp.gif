   function update_cart() {

    $.post("cart/backend/update_cart.php", {}, on_success);

    function on_success(data)

    {

     $('#small_cart').html(data);

    }

   }

   function remove_from_cart(product_id) {

    $.post("cart/backend/remove_from_cart.php", {product_id: product_id}, update_cart_interface);

   }

   function update_product_count(product_id, count) {

    $.post("cart/backend/update_product_count.php", {product_id: product_id, count: count}, update_cart_interface);

   }

   function update_cart_interface() {

    $.post("cart/backend/cart_interface.php", {}, on_success);

    function on_success(data)

    {

     $('#cart_interface').html(data);

    }

   }

  </script>

